{
  "expo": {
    "name": "Blind Write",
    "slug": "BlindWrite",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tkgz.blindwrite"
    },
    "description": "Simple prototype app to teach blind people how to write with their phones.",
    "githubUrl": "https://github.com/TKGZ/BlindWrite"
  }
}
